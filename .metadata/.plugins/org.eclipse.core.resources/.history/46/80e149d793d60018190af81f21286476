<script>
    function Checkpwd() 
{ 
var pwd=  document.getElementById("pwd").value;
var pwdchk=  document.getElementById("pwdchk").value;
if(pwdchk=pwd) 
{ 
alert('correct!')
return true;
}
else
{ 
alert('Wrong...! retry')
return false;
}
}
</script>
